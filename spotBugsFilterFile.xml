<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
        xmlns="https://github.com/spotbugs/filter/4.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/spotbugs/filter/4.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/4.0.0/spotbugs/etc/findbugsfilter.xsd">
    <Match>
        <Bug pattern="EI_EXPOSE_REP2, EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="pl.fairit.somedayiwill.book.booksearch.GoogleBooksService"/>
        <Method name="searchBooks"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
    <Match>
        <Class name="pl.fairit.somedayiwill.movie.moviesearch.MDBMovieService"/>
        <Method name="getGenresMap"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
    <Match>
        <Class name="pl.fairit.somedayiwill.movie.moviesearch.MDBMovieService"/>
        <Method name="searchMoviesByTitle"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
</FindBugsFilter>
